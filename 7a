//WAP having a class called 'student' with two data members 'roll_no' and marks including two member functions setvalue() and getvalue() for setting and displaying the value of data members call these functions from main() by creating two objects of class.

#include<iostream>
using namespace std;
class student
{
public:
    int roll_no,marks;
void setdata(int a,int b)
{
 roll_no=a;
marks=b;
}
void showdata()
{
cout<<"The Roll no of the student is: "<<roll_no<<endl<<"The marks is: "<<marks;
}
};
int main()
{
    student obj1;
    obj1.setdata(12,34);
    obj1.showdata();
    return 0;
}
